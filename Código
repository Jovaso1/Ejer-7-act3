    private void botoncalcularActionPerformed(java.awt.event.ActionEvent evt) {                                              
        double valorA, valorB;
        double mensaje;
        
        valorA = Double.parseDouble(txtvalorA.getText());
        valorB = Double.parseDouble(txtvalorB.getText());
        mensaje = Mayormenorigual.calcmayormenor(valorA, valorB);
        
        if (mensaje == valorA){
            txtmensaje.setText("El valor A es mayor que B");
        }else if (mensaje == valorB){
            txtmensaje.setText("El valor B es mayor que A");
        }else{
            txtmensaje.setText("El valor A es igual que B");
        }
        
    }                                             

    private void botonborrarActionPerformed(java.awt.event.ActionEvent evt) {                                            
        txtvalorA.setText("");
        txtvalorB.setText("");        
        txtmensaje.setText("");
    }                                           

    private void botonsalirActionPerformed(java.awt.event.ActionEvent evt) {                                           
        // TODO add your handling code here:
        dispose();
    }                                          

